<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="686.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javasystemapp.CustomerLoginPageController">
    <children>
        <ImageView fitHeight="400.0" fitWidth="726.0" layoutX="-38.0">
            <image>
                <Image url="@../../../assets/4.jpg" />
            </image>
        </ImageView>
        <TextField fx:id="tfUsername" layoutX="435.0" layoutY="151.0" style="-fx-background-radius: 5;" />
        <PasswordField fx:id="tfPassword" layoutX="435.0" layoutY="219.0" promptText="Enter Your Password" style="-fx-background-radius: 5;" />
        <Label layoutX="355.0" layoutY="151.0" text="Username" textFill="WHITE">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label layoutX="357.0" layoutY="221.0" text="Password" textFill="WHITE">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Button fx:id="btnLogin" layoutX="435.0" layoutY="292.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #ffdb49;" text="Login" />
        <Button fx:id="btnClear" layoutX="559.0" layoutY="289.0" mnemonicParsing="false" onAction="#clear" prefHeight="25.0" prefWidth="63.0" style="-fx-background-color: #FC4F00;" text="Clear" textFill="WHITE" />
        <Text layoutX="81.0" layoutY="321.0" strokeType="OUTSIDE" strokeWidth="0.0" />
        <Hyperlink fx:id="goToCreate" layoutX="473.0" layoutY="363.0" onAction="#goToCreate" text="Create Account" />
        <Label fx:id="lblErrorMessage" layoutX="384.0" layoutY="120.0" prefHeight="17.0" prefWidth="278.0" textFill="#d41f1f" />
        <Text fill="#ffdb49" layoutX="384.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="MABSL|Login Page">
            <font>
                <Font name="Tahoma Bold" size="22.0" />
            </font>
        </Text>
        <ImageView fx:id="eyeIcon" fitHeight="27.0" fitWidth="16.0" layoutX="592.0" layoutY="227.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../assets/eye_icon.png" />
            </image>
        </ImageView>
        <Label layoutX="445.0" layoutY="342.0" text="Don't have an account?" textFill="WHITE" />
    </children>
</AnchorPane>
